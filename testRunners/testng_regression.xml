<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Browser Stack Test automtion regression suite"
	thread-count="3" parallel="tests" verbose="3">


	<test name="Browser Stack Test - FF">
		<parameter name="os" value="Windows" />
		<parameter name="os_version" value="10" />
		<parameter name="browser" value="Firefox" />
		<parameter name="browser_version" value="80.0" />
		
				<classes>
			<class name="com.myTests.BrowserStackTest" />
		</classes>

	</test>

	<test name="Browser Stack Test - Chrome">
		<parameter name="os" value="Windows" />
		<parameter name="os_version" value="10" />
		<parameter name="browser" value="Chrome" />
		<parameter name="browser_version" value="81.0" />
		
	
		<classes>
			<class name="com.myTests.BrowserStackTest" />
		</classes>

	</test>










</suite>
